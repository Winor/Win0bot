{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "src",
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "sourceMap": true,
    "keyofStringsOnly": true
  },
  "include": ["src/*.json", "src"],
  "exclude": ["src/robot_modules/old/*"]
}